<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.springframework.org/schema/beans'
    xsi:schemaLocation='http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd'>
	
    <bean id="wicketApplication" class="nl.mpi.archivetree.wicket.ArchiveTreeApplication" />
	
    <bean id="workspaceService" class="nl.mpi.archivetree.model.mock.MockWorkspaceService">
	<constructor-arg ref="mockCreatedWorkspace" />
    </bean>
    
    <bean id="mockCreatedWorkspace" class="nl.mpi.archivetree.model.mock.MockWorkspace">
	<property name="workspaceID" value="1"/>
    </bean>
</beans>
